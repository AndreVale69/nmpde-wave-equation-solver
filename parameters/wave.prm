subsection Study
  set enable_dissipation = true
  set dissipation_every = 1
  set dissipation_csv = dissipation.csv

    set enable_modal = true
    set modal_every = 1
    set modal_csv = modal.csv
    set modal_k = 1
    set modal_l = 1
end

subsection Mesh
  set mesh_file = ../mesh/square_structured.geo
  set degree    = 1
end

subsection Time
  set T      = 1.0
  set dt     = 0.01
  set theta  = 1.0
  set scheme = theta
end

subsection Output
  set every = 1
end
